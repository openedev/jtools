## git command sudo alias
sudo alias gs='git status'
sudo alias gb='git branch'
sudo alias gb+='git branch -r'
sudo alias ga='git add'
sudo alias ga+='git add -A'
sudo alias gc='git checkout'
sudo alias gc+='git checkout -b'
sudo alias gcs='git commit -s'
sudo alias gcsa='git commit -s --amend'
sudo alias gr='git rebase -i'
sudo alias grc='git rebase --continue'
sudo alias gf='git format-patch -M'
sudo alias gf+='git format-patch -M --cover-letter'
sudo alias gl='git log'
sudo alias gp='git pull'
sudo alias grh='git reset --hard'
sudo alias gptom='gp git://git.denx.de/u-boot.git master:master'
sudo alias gpush='git push -v spi-write master:master'
sudo alias gtags='git push -v spi-write --tags'
sudo alias gcount='git shortlog -s -n'
sudo alias gcount+='git shortlog -s -n --author="Jagan"'
