## git command alias
alias gs='sudo git status'
alias gb='sudo git branch'
alias gb+='sudo git branch -r'
alias ga='sudo git add'
alias ga+='sudo git add -A'
alias gc='sudo git checkout'
alias gc+='sudo git checkout -b'
alias gcs='sudo git commit -s'
alias gcsa='sudo git commit -s --amend'
alias gr='sudo git rebase -i'
alias grc='sudo git rebase --continue'
alias gf='sudo git format-patch -M'
alias gf+='sudo git format-patch -M --cover-letter'
alias gl='sudo git log'
alias gp='sudo git pull'
alias grh='sudo git reset --hard'
alias gptom='gp sudo git://git.denx.de/u-boot.git master:master'
alias gpush='sudo git push -v spi-write master:master'
alias gtags='sudo git push -v spi-write --tags'
alias gcount='sudo git shortlog -s -n'
alias gcount+='sudo git shortlog -s -n --author="Jagan"'
