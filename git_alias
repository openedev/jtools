## git command alias
alias gs='git status'
alias gb='git branch'
alias gb+='git branch -r'
alias ga='git add'
alias ga+='git add -A'
alias gc='git checkout'
alias gc+='git checkout -b'
alias gcs='git commit -s'
alias gcsa='git commit -s --amend'
alias gr='git rebase -i'
alias grc='git rebase --continue'
alias gf='git format-patch -M'
alias gf+='git format-patch -M --cover-letter'
alias gl='git log'
alias gp='git pull'
alias grh='git reset --hard'
alias gptom='gp git://git.denx.de/u-boot.git master:master'
alias gpush='git push -v spi-write master:master'
alias gtags='git push -v spi-write --tags'
alias gcount='git shortlog -s -n'
alias gcount+='git shortlog -s -n --author="Jagan"'
